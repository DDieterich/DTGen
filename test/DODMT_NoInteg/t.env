
export OWNERNAME=TMTSNDOD
export OWNERPASS=TMTSNDOD
export USERNAME=TMTUNDOD
export USERPASS=TMTUNDOD

# Set Connect Strings
export OWNER_CONNECT_STRING=${OWNERNAME}/${OWNERPASS}
if [ ${TNS_ALIAS-NULL} != "NULL" ]
then
   OWNER_CONNECT_STRING=${OWNER_CONNECT_STRING}@${TNS_ALIAS}
fi
export USER_CONNECT_STRING=${USERNAME}/${USERPASS}
if [ ${TNS_ALIAS-NULL} != "NULL" ]
then
   USER_CONNECT_STRING=${USER_CONNECT_STRING}@${TNS_ALIAS}
fi

if [ ${1-NULL} = -p ]
then
   echo "OWNERNAME = ${OWNERNAME}"
   echo "OWNERPASS = ${OWNERPASS}"
   echo "USERNAME  = ${USERNAME}"
   echo "USERPASS  = ${USERPASS}"
   echo "OWNER_CONNECT_STRING = ${OWNER_CONNECT_STRING}"
   echo "USER_CONNECT_STRING  = ${USER_CONNECT_STRING}"
fi
