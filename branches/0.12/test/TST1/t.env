
export DB_SCHEMA=T1DBS
export DB_SPASS=T1DBS
export DB_USER=T1DBU
export DB_UPASS=T1DBU
export MT_SCHEMA=T1MTS
export MT_SPASS=T1MTS
export MT_USER=T1MTU
export MT_UPASS=T1MTU

# Set Connect Strings
export DB_SCHEMA_CONNECT=${DB_SCHEMA}/${DB_SPASS}
if [ ${TNS_ALIAS-NULL} != "NULL" ]
then
   DB_SCHEMA_CONNECT=${DB_SCHEMA_CONNECT}@${TNS_ALIAS}
fi
export DB_USER_CONNECT=${DB_USER}/${DB_UPASS}
if [ ${TNS_ALIAS-NULL} != "NULL" ]
then
   DB_USER_CONNECT=${DB_USER_CONNECT}@${TNS_ALIAS}
fi
export MT_SCHEMA_CONNECT=${MT_SCHEMA}/${MT_SPASS}
if [ ${TNS_ALIAS-NULL} != "NULL" ]
then
   MT_SCHEMA_CONNECT=${MT_SCHEMA_CONNECT}@${TNS_ALIAS}
fi
export MT_USER_CONNECT=${MT_USER}/${MT_UPASS}
if [ ${TNS_ALIAS-NULL} != "NULL" ]
then
   MT_USER_CONNECT=${MT_USER_CONNECT}@${TNS_ALIAS}
fi

if [ ${1-NULL} = -p ]
then
   echo "DB_SCHEMA = ${DB_SCHEMA}"
   echo "DB_SPASS = ${DB_SPASS}"
   echo "DB_SCHEMA_CONNECT = ${DB_SCHEMA_CONNECT}"
   echo "DB_USER  = ${DB_USER}"
   echo "DB_UPASS  = ${DB_UPASS}"
   echo "DB_USER_CONNECT  = ${DB_USER_CONNECT}"
   echo "MT_SCHEMA = ${MT_SCHEMA}"
   echo "MT_SPASS = ${MT_SPASS}"
   echo "MT_SCHEMA_CONNECT = ${MT_SCHEMA_CONNECT}"
   echo "MT_USER  = ${MT_USER}"
   echo "MT_UPASS  = ${MT_UPASS}"
   echo "MT_USER_CONNECT  = ${MT_USER_CONNECT}"
fi
